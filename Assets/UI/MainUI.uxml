            </ui:ScrollView>
        </ui:VisualElement>
        <ui:VisualElement style="flex-grow: 1; flex-direction: row; height: 25px; max-height: 25px; align-self: auto; justify-content: center; margin-bottom: 5px;">
            <ui:Button text="B" parse-escape-sequences="true" display-tooltip-when-elided="true" name="BuildingsButton" style="width: 20px;" />
            <ui:Button text="U" parse-escape-sequences="true" display-tooltip-when-elided="true" name="HerosButton" style="width: 20px;" />
            <ui:Button text="R" parse-escape-sequences="true" display-tooltip-when-elided="true" name="ResourcesButton" style="align-self: auto; width: 20px;" />
            <ui:Button text="X" parse-escape-sequences="true" display-tooltip-when-elided="true" name="CloseDetailButton" style="width: 20px; align-self: auto;" />
